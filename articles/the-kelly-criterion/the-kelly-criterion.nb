Cell[BoxData[
 RowBox[{"Block", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{"simulations", ",", " ", "simulateBettor"}], "}"}], ",", 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"simulations", ":=", 
     RowBox[{
      RowBox[{"RandomVariate", "[", 
       RowBox[{
        RowBox[{"BernoulliDistribution", "[", "0.6", "]"}], ",", "200"}], 
       "]"}], "//", 
      RowBox[{"Map", "[", 
       RowBox[{
        RowBox[{"#", "*", "2"}], "&"}], "]"}]}]}], ";", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"simulateBettor", "[", "f_", "]"}], ":=", 
     RowBox[{"FoldList", "[", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"Function", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"wealth", ",", " ", "outcome"}], "}"}], ",", 
         RowBox[{"wealth", "*", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"f", "*", "outcome"}], "+", 
            RowBox[{"(", 
             RowBox[{"1", "-", "f"}], ")"}]}], ")"}]}]}], "]"}], 
       "\[IndentingNewLine]", ",", "1", "\[IndentingNewLine]", ",", 
       "simulations"}], "\[IndentingNewLine]", "]"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"Row", "[", "\[IndentingNewLine]", 
     RowBox[{"{", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"ListLinePlot", "[", 
        RowBox[{
         RowBox[{"{", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"simulateBettor", "[", "0.1", "]"}], ",", 
           "\[IndentingNewLine]", 
           RowBox[{"simulateBettor", "[", "0.2", "]"}], ",", 
           "\[IndentingNewLine]", 

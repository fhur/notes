           "\[IndentingNewLine]", 
           RowBox[{"simulateBettor", "[", "0.5", "]"}], ",", 
           "\[IndentingNewLine]", 
           RowBox[{"simulateBettor", "[", "0.7", "]"}]}], 
          "\[IndentingNewLine]", "}"}], ",", "\[IndentingNewLine]", 
         RowBox[{"PlotLabel", "\[Rule]", "\"\<Wealth per step\>\""}], ",", 
         "\[IndentingNewLine]", 
         RowBox[{"ImageSize", "\[Rule]", "Medium"}]}], "\[IndentingNewLine]", 
        "]"}], ",", "\[IndentingNewLine]", 
       RowBox[{"ListLogPlot", "[", 
        RowBox[{
         RowBox[{"{", "\[IndentingNewLine]", 
          RowBox[{
